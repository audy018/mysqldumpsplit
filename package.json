    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "coveralls": "^2.11.8",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "isparta": "^4.0.0",
